<Window x:Class="Formula1.TimeLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Formula1"
        mc:Ignorable="d"
        Title="Timeline" Height="450" Width="800" Loaded="Window_Loaded" ResizeMode="NoResize" ShowInTaskbar="False" ContentRendered="Window_ContentRendered">
    <ScrollViewer HorizontalScrollBarVisibility="Auto" ScrollChanged="ChartScrollViewer_ScrollChanged">
        <Canvas x:Name="ChartCanvas" Background="Snow">
        </Canvas>
    </ScrollViewer>
</Window>
